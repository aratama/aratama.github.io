// Bar chimes

<group>
ampeg_release=1

<region>
sample=samples/percussion/bar_chimes-a.mp3
lokey=c3
hikey=c3
pitch_keycenter=c3

<region>
sample=samples/percussion/bar_chimes-b.mp3
lokey=d3
hikey=d3
pitch_keycenter=d3

<region>
sample=samples/percussion/bar_chimes-c.mp3
lokey=e3
hikey=e3
pitch_keycenter=e3

// Tambourine

<group>
loop_mode=one_shot

<region>
sample=samples/percussion/tambourine-hit-f.mp3
lokey=f3
hikey=f3
pitch_keycenter=f3
hivel=127
lovel=80

<region>
sample=samples/percussion/tambourine-hit-mf.mp3
lokey=f3
hikey=f3
pitch_keycenter=f3
hivel=79
lovel=0

<group>
ampeg_release=1

<region>
sample=samples/percussion/tambourine-shake.mp3
lokey=g3
hikey=g3
pitch_keycenter=g3

<region>
sample=samples/percussion/tambourine-roll.mp3
lokey=a3
hikey=a3
pitch_keycenter=a3

